{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle" : ["warn", { 
            "allow": [ "_id" ]
        }]
    },
    "ignorePatterns": [
        "node_modules/"
    ]
}
